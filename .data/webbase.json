{"_id":"169ecfb0-2916-11ee-ad92-6bd31f953e80","type":"Webo","status":"U","name":{"en":"Hello World"},"children":[{"_id":"169ecfb1-2916-11ee-ad92-6bd31f953e80","_idParent":"169ecfb0-2916-11ee-ad92-6bd31f953e80","type":"Page","status":"U","name":{"en":"Home"},"slug":{"en":"home"},"children":[{"_id":"169ecfb2-2916-11ee-ad92-6bd31f953e80","_idParent":"169ecfb1-2916-11ee-ad92-6bd31f953e80","type":"Content","status":"M","name":{"en":"Customers"},"slug":{"en":"customers"},"visibility":{"guests":false},"subtype":"Table","cssClass":"stwTable","section":"main","sequence":"1","dsn":"mysqlsample","query":"select * from employees limit 10","params":"","layout":{"en":"\\s('caption=\"Customers\" collapsible ')\n\\A('style=\"color:red\" ') \nl('Name')a('sales')pff\nl('Ext.')f\nl('Email')a('mailto:@@Email')f\n"}},{"_id":"0c6511f0-aff6-11ee-86f2-1733f9fc2f8d","_idParent":"169ecfb1-2916-11ee-ad92-6bd31f953e80","type":"Content","status":"M","name":{"en":"DB Schema"},"slug":{"en":"dbschema"},"visibility":{"guests":false},"subtype":"Tree","cssClass":"stwTree","section":"main","sequence":"10","dsn":"mysqlsample","query":"SELECT 'TABLES' as TYPE, 'TABLES' as NAME, null as rows, '' as DATA_TYPE, '' as COLUMN_TYPE, '' as COLUMN_KEY, 'TABLE' as PATH\nUNION\nSELECT 'TABLE', T.TABLE_NAME, T.TABLE_ROWS, '', '', '', concat_ws('/', 'TABLE', T.TABLE_NAME)\n    FROM information_schema.tables T \n    WHERE T.TABLE_SCHEMA='qt3ul9nb_sampledb' and T.TABLE_TYPE = 'BASE TABLE'\nUNION\nSELECT 'COLUMN', C.COLUMN_NAME, null, C.DATA_TYPE, C.COLUMN_TYPE, C.COLUMN_KEY, concat_ws('/', 'TABLE', T.TABLE_NAME, C.COLUMN_NAME)\n    FROM information_schema.tables T inner join information_schema.columns C on T.TABLE_SCHEMA = C.TABLE_SCHEMA and T.TABLE_NAME = C.TABLE_NAME \n    WHERE T.TABLE_SCHEMA='qt3ul9nb_sampledb' and T.TABLE_TYPE = 'BASE TABLE'\nUNION\nSELECT 'VIEWS', 'VIEWS', null, '', '', '', 'VIEW'\nUNION\nSELECT 'VIEW', V.TABLE_NAME, null, '', '', '', concat_ws('/', 'VIEW', V.TABLE_NAME)\n    FROM information_schema.views V\n    WHERE V.TABLE_SCHEMA='qt3ul9nb_sampledb'\nUNION\nSELECT 'COLUMN', C.COLUMN_NAME, null, C.DATA_TYPE, C.COLUMN_TYPE, C.COLUMN_KEY, concat_ws('/', 'VIEW', V.TABLE_NAME, C.COLUMN_NAME)\n    FROM information_schema.views V inner join information_schema.columns C on V.TABLE_SCHEMA = C.TABLE_SCHEMA and V.TABLE_NAME = C.TABLE_NAME\n    WHERE V.TABLE_SCHEMA='qt3ul9nb_sampledb'\nUNION\nSELECT 'ROUTINES', 'ROUTINES', null, '', '', '', 'ROUTINE'\nUNION\nSELECT distinct concat(R.ROUTINE_TYPE, 'S'), concat(R.ROUTINE_TYPE, 'S'), null, '', '', '', concat_ws('/', 'ROUTINE', R.ROUTINE_TYPE)\n    FROM information_schema.ROUTINES R\n    WHERE R.ROUTINE_SCHEMA='qt3ul9nb_sampledb'\nUNION\nSELECT R.ROUTINE_TYPE, R.ROUTINE_NAME, null, '', '', '', concat_ws('/', 'ROUTINE', R.ROUTINE_TYPE, R.ROUTINE_NAME)\n    FROM information_schema.ROUTINES R\n    WHERE R.ROUTINE_SCHEMA='qt3ul9nb_sampledb'\nUNION\nSELECT 'PARAMETER', ifnull(P.PARAMETER_NAME, 'RETURN'), null, P.DATA_TYPE, P.DTD_IDENTIFIER, ifnull(P.PARAMETER_MODE, ''), concat_ws('/', 'ROUTINE', R.ROUTINE_TYPE, R.ROUTINE_NAME, ifnull(P.PARAMETER_NAME, 'RETURN'))\n    FROM information_schema.ROUTINES R left join information_schema.PARAMETERS P ON R.ROUTINE_SCHEMA = P.SPECIFIC_SCHEMA and R.ROUTINE_NAME = P.SPECIFIC_NAME\n    WHERE R.ROUTINE_SCHEMA='qt3ul9nb_sampledb'","params":"","layout":{"en":"\\s('caption=\"DB Schema\" key=\"path\"')\nt('<span><i class=\"fas fa-fw fa-table\"></i></span>')>a('sales')p<ff>ff"}},{"_id":"6b70fab0-2aa9-11ee-956d-479821047fbc","_idParent":"169ecfb1-2916-11ee-ad92-6bd31f953e80","type":"Content","status":"M","name":{"en":"Logoff"},"slug":{"en":"logoff"},"visibility":{"guests":false},"subtype":"Form","cssClass":"stwForm","section":"main","sequence":"2","dsn":"","query":"","params":"","layout":{"en":"t('\n<form action=\"/stw/logoff\" method=\"post\">\n<button type=\"submit\">Logoff</button>\n</form>')"}},{"_id":"4afeeae0-2a4e-11ee-9a5d-8ff6b23b06b9","_idParent":"169ecfb1-2916-11ee-ad92-6bd31f953e80